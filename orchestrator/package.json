{
  "name": "orchestrator",
  "version": "1.1.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" ",
    "lint-fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "precommit": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "compile-ts": "tsc --p ."
  },
  "dependencies": {
    "@hapi/joi": "15.1.1",
    "@nestjs/common": "6.10.1",
    "@nestjs/core": "6.10.1",
    "@nestjs/mongoose": "6.1.2",
    "@nestjs/platform-express": "6.10.1",
    "@nestjs/platform-fastify": "6.10.1",
    "@nestjs/swagger": "^4.5.12",
    "async": "^3.2.0",
    "class-transformer": "0.2.3",
    "class-validator": "0.10.0",
    "config": "3.2.2",
    "connect-redis": "^4.0.4",
    "csurf": "1.10.0",
    "deepcopy": "2.0.0",
    "delay": "4.3.0",
    "dotenv": "8.1.0",
    "fastify": "2.9.0",
    "fastify-compress": "0.11.0",
    "fastify-cookie": "3.1.0",
    "fastify-rate-limit": "^4.0.0",
    "fastify-session": "3.0.0",
    "fastify-swagger": "^2.4.0",
    "generate-password": "1.4.2",
    "helmet": "3.21.2",
    "mongoose": "5.9.20",
    "node-seaweedfs": "^1.4.0",
    "pg": "8.3.0",
    "redis": "^3.0.2",
    "reflect-metadata": "0.1.12",
    "rimraf": "2.6.2",
    "rxjs": "6.3.3",
    "saslprep": "1.0.3",
    "sleep": "^6.1.0",
    "sprintf-js": "1.1.2",
    "stripe": "^8.19.0",
    "underscore": "1.9.1",
    "uuid-validate": "0.0.3"
  },
  "devDependencies": {
    "@nestjs/testing": "6.8.5",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/mongoose": "5.5.17",
    "@types/node": "12.11.1",
    "@types/supertest": "2.0.7",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "4.2.5",
    "jest": "^24.7.1",
    "lint-staged": "10.2.11",
    "prettier": "1.17.0",
    "saslprep": "1.0.3",
    "start-server-webpack-plugin": "^2.2.5",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-loader": "^7.0.5",
    "ts-node": "8.4.1",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
